<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.EstSendMapper">

  <insert id="est_send" parameterType="SendDTO">
  insert into est_send values(#{est_send_num.nextval}, #{user_id}, sysdate, #{send_country}, #{send_place}, #{send_s_date}, #{send_e_date}, #{send_budget}, #{send_content})
  </insert>
  
</mapper>