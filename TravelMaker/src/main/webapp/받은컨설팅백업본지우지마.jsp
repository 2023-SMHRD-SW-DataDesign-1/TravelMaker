	<script>

    let markers = [];
    let markerCount = 0;

    function createHTMLMarker(map, position, content) {
        const customMarker = new google.maps.Marker({
            position: position,
            map: map,
            icon: {
                url: 'data:image/svg+xml;charset=UTF-8,' + encodeURIComponent(`<svg width="200" height="200" xmlns="http://www.w3.org/2000/svg"><image href="data:image/png;base64," height="200" width="200" /><text x="2" y="-27" font-size="80" font-weight="bold" text-anchor="middle" alignment-baseline="middle" transform="translate(100, 100)">${content}</text></svg>`),
                scaledSize: new google.maps.Size(40, 40),
                anchor: new google.maps.Point(15, 15)
            },
            draggable: false
        });
        return customMarker;
    }

    function initMap() {
        let map;
        

        <% for (int i = 0; i < cons_list.size(); i++) { %>
            const latPositions_<%= i %> = '<%= cons_list.get(i).getCons_lat() %>'.split(';');
            const lngPositions_<%= i %> = '<%= cons_list.get(i).getCons_lng() %>'.split(';');
            const mapContainer_<%= i %> = document.getElementById('map_<%= i %>');
            const lat_<%= i %> = parseFloat(latPositions_<%= i %>[0]);
            const lng_<%= i %> = parseFloat(lngPositions_<%= i %>[0]);
            const defaultPosition_<%= i %> = { lat: lat_<%= i %>, lng: lng_<%= i %> };

            const map_<%= i %> = new google.maps.Map(mapContainer_<%= i %>, {
                center: defaultPosition_<%= i %>,
                zoom: 15
            });

            for (let j = 0; j < latPositions_<%= i %>.length; j++) {
                markerCount++;
                const lat = parseFloat(latPositions_<%= i %>[j]);
                const lng = parseFloat(lngPositions_<%= i %>[j]);
                const position = { lat: lat, lng: lng };
                const marker = createHTMLMarker(map_<%= i %>, position, markerCount.toString());
                markers.push(marker);
            }
            markerCount = 0; // 초기화
        <% } %>
    }

    // initMap 함수를 콜백으로 등록
    document.addEventListener('DOMContentLoaded', initMap);
    </script>

